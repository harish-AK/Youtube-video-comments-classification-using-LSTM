    "import re\n",
    "import html\n",
    "import emoji\n",
    "\n",
    "def clean_comment(comment):\n",
    "    comment = html.unescape(comment) # decode html entities\n",
    "    \n",
    "    comment = re.sub(r'<.*?>', '', comment) # remove hmtl tags\n",
    "    \n",
    "    comment = re.sub(r'\\d+:\\d+', '', comment) # remove timestamps\n",
    "    \n",
    "    comment = emoji.replace_emoji(comment, '') # remove emojis\n",
    "    \n",
    "    comment = re.sub(r'http\\S+|www.\\S+', '', comment) # remove urls\n",
    "    \n",
    "    comment = ' '.join(comment.split()) # remove whitespaces\n",
    "    \n",
    "    comment = re.sub(r'[^a-zA-Z0-9\\s.,!?]', '', comment) # remove special chars\n",
    "    \n",
    "    return comment.strip()\n",
    "\n",
    "cleaned_cmts = []\n",
    "\n",
    "for item in video_response[\"items\"]:\n",
    "    comment = item[\"snippet\"][\"topLevelComment\"]\n",
    "    uncleaned_cmt = comment['snippet'][\"textDisplay\"]\n",
    "    \n",
    "    cleaned_cmt = clean_comment(uncleaned_cmt)\n",
    "    \n",
    "    cleaned_cmts.append(cleaned_cmt)\n",
    "\n",
    "print(\"\\nCleaned comments:\")\n",
    "for cmt in cleaned_cmts:\n",
    "    print(cmt)"
   ]
